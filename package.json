{
  "name": "biz-landing",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:pl": "ng serve --port 4200 --configuration pl",
    "start:uk": "ng serve --port 4200 --configuration uk",
    "start:da": "ng serve --port 4200 --configuration da",
    "start": "ng serve --port 4200",
    "start-only": "ng serve",
    "build": "ng build",
    "build:develop": "ng build --configuration develop",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run biz-landing:serve-ssr",
    "serve:ssr": "node dist/biz-landing/server/main.js",
    "build:ssr": "ng build && ng run biz-landing:server",
    "prerender": "ng run biz-landing:prerender",
    "i18n:extract": "ng extract-i18n",
    "build:firebase:development": "ng build --configuration development --output-path=./environments/develop/firebase/dist && npm run build:firebase:prepare_development_base --output-path=./environments/develop/firebase/dist",
    "build:firebase:prepare_development_base": "ng build --configuration prepare_development_base --output-path=./environments/develop/firebase/dist",
    "build:firebase:production": "ng build --configuration production --output-path=./environments/production/firebase/dist && npm run build:firebase:prepare_production_base --output-path=./environments/production/firebase/dist",
    "build:firebase:prepare_production_base": "ng build --configuration prepare_production_base --output-path=./environments/production/firebase/dist"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "19.2.3",
    "@angular/compiler": "19.2.3",
    "@angular/core": "19.2.3",
    "@angular/fire": "19.0.0",
    "@angular/forms": "19.2.3",
    "@angular/platform-browser": "19.2.3",
    "@angular/platform-browser-dynamic": "19.2.3",
    "@angular/platform-server": "19.2.3",
    "@angular/router": "19.2.3",
    "@angular/ssr": "19.2.4",
    "@ng-icons/bootstrap-icons": "31.2.0",
    "@ng-icons/core": "31.2.0",
    "express": "4.21.2",
    "intl-tel-input": "25.3.0",
    "js-confetti": "0.12.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.4",
    "@angular/cli": "19.2.4",
    "@angular/compiler-cli": "19.2.3",
    "@angular/localize": "19.2.3",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/typography": "0.5.16",
    "@types/express": "5.0.1",
    "@types/node": "22.13.11",
    "autoprefixer": "10.4.21",
    "browser-sync": "3.0.3",
    "ng-extract-i18n-merge": "2.14.3",
    "postcss": "8.5.3",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.2"
  }
}
