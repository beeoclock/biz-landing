{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "biz-landing": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "i18n": {
        "sourceLocale": "en-US",
        "locales": {
          "da": {
            "translation": "src/locale/messages.da.xlf"
          },
          "uk": {
            "translation": "src/locale/messages.uk.xlf"
          },
          "pl": {
            "translation": "src/locale/messages.pl.xlf"
          }
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "allowedCommonJsDependencies": [
              "intl-tel-input"
            ],
            "outputPath": "dist/biz-landing",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "@angular/localize/init"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "robots.txt",
                "input": "environments/develop/",
                "output": "./"
              }
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "server": "src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "server.ts"
            },
            "localize": true,
            "i18nMissingTranslation": "warning"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "750kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "environments/production/environment.ts"
                }
              ],
              "prerender": {
                "discoverRoutes": true,
                "routesFile": "environments/production/routes.txt"
              },
              "assets": [
                "src/favicon.ico",
                "src/assets",
                {
                  "glob": "robots.txt",
                  "input": "environments/production/",
                  "output": "./"
                }
              ],
              "outputHashing": "all"
            },
            "prepare_production_base": {
              "localize": false,
              "deleteOutputPath": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "environments/production/environment.ts"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                {
                  "glob": "robots.txt",
                  "input": "environments/production/",
                  "output": "./"
                }
              ],
              "prerender": {
                "discoverRoutes": true,
                "routesFile": "environments/production/routes.txt"
              },
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "environments/develop/environment.ts"
                }
              ],
              "prerender": {
                "discoverRoutes": true,
                "routesFile": "environments/develop/routes.txt"
              }
            },
            "prepare_development_base": {
              "localize": false,
              "deleteOutputPath": false,
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "environments/develop/environment.ts"
                }
              ],
              "prerender": {
                "discoverRoutes": true,
                "routesFile": "environments/develop/routes.txt"
              }
            },
            "develop": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "environments/develop/environment.ts"
                }
              ]
            },
            "en-US": {
              "localize": ["en-US"]
            },
            "pl": {
              "localize": ["pl"]
            },
            "da": {
              "localize": ["da"]
            },
            "uk": {
              "localize": ["uk"]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "biz-landing:build:production"
            },
            "development": {
              "buildTarget": "biz-landing:build:development"
            },
            "da": {
              "buildTarget": "biz-landing:build:development,da"
            },
            "uk": {
              "buildTarget": "biz-landing:build:development,uk"
            },
            "pl": {
              "buildTarget": "biz-landing:build:development,pl"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "ng-extract-i18n-merge:ng-extract-i18n-merge",
          "options": {
            "buildTarget": "biz-landing:build",
            "format": "xlf2",
            "outputPath": "src/locale",
            "targetFiles": [
              "messages.da.xlf",
              "messages.uk.xlf",
              "messages.pl.xlf"
            ]
          }
        }
      }
    }
  }
}
